{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ESNext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "removeComments": true,
        "outDir": "./build"
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules/**/*"
    ]
}