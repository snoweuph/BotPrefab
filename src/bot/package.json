    "@discordjs/rest": "^0.3.0",
    "canvas": "^2.9.1",
    "discord-api-types": "^0.29.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "module-alias": "^2.2.2",
    "mysql2": "^2.3.3"
  },
  "devDependencies": {
    "@swc/core": "^1.2.164",
    "@swc/helpers": "^0.3.8",
    "@types/events": "^3.0.0",
    "@types/module-alias": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
