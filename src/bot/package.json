{
  "name": "bot",
  "version": "0.0.1",
  "description": "",
  "main": "bot.ts",
  "scripts": {
    "start:bot": "node build/bot.js",
    "start:shard": "node build/sharded.js",
    "start:reg": "node build/register.js",
    "build": "tsc  && tsc-alias",
    "build:doc": "typedoc",
